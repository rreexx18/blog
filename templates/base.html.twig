
        {% include 'header.html.twig' %}

        <!-- Begin page content -->
        <main role="main" class="flex-shrink-0">
            <div class="container">
                {% block body %}{% endblock %}
                <div id="fmessage">
                    {% for label,messages in app.flashes %}
                        {% for message in messages %}
                            <div class="alert alert-{{ label }}">
                                {{ message }}
                            </div>
                        {% endfor %}
                    {% endfor %}
                </div>
            </div>
        </main>

        {% include 'footer.html.twig' %}
    </body>
</html>
